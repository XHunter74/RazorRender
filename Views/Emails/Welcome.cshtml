@using RazorRender.Models
@model EmailModel
<h1 style="font-family: Arial, sans-serif; color: #4CAF50;">Welcome, @Model.Name!</h1>
<p style="font-family: Arial, sans-serif; color: #555;">Your email: <strong>@Model.Email</strong></p>
<p style="font-family: Arial, sans-serif; color: #555;">Your items:</p>
<ul style="font-family: Arial, sans-serif; color: #333; list-style-type: square; padding-left: 20px;">
  @foreach (var item in Model.Items)
  {
      <li style="font-family: Arial, sans-serif; color: #4CAF50;">@item</li>
  }
</ul>